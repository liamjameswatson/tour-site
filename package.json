{
  "name": "nature-tours",
  "version": "1.0.0",
  "description": "a tour booking website, built with node, express and mongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "set NODE_ENV=development && nodemon server.js",
    "start:prod": "set NODE_ENV=production && nodemon server.js",
    "debug": "ndb server.js",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled",
    "watch:js": "parcel serve --no-hmr ./public/js/index.js --dist-dir ./public/js/bundled"
  },
